int main()
{
    int n,t,i,count;
   // freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
   cin>>t;
   while(t--)
{
    count=0;
    cin>>n;
     int arr[n+1];
    for(i=0;i<n;i++)
    cin>>arr[i];
    int &max = *max_element(arr,arr+n );
