#include<bits/stdc++.h>
using namespace std;
int main() {
  ios::sync_with_stdio(false);
  cin.tie(0);
  long long t,i,sum,remainder,j,sum1,arr[55],diff;
 cin >> t;
  while (t--) {

        long long n;
        cin>>n;

sum=0;
for(i=0;i<n;i++)
{
    cin>>arr[i];
}
sort(arr, arr + n);
for (int i = 0; i < n; i++)
{diff=abs(arr[0]-arr[i]);
sum+=diff;
  }
   cout<<sum<<endl;
   //cout<<arr[n-1]<<endl;
   //else   cout<<"no"<<endl;
}
  return 0;
}
