#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long n,t,k,ans;
   // freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
   cin>>t;
   while(t--)
{
    cin>>n>>k;
    if(n>=k){
        if(n%k==0)ans=1;
        else ans=2;
    }
    else {
        if(k%n==0)ans=k/n;
        else ans=k/n+1;
    }


    cout<<ans<<endl;

}




    return 0;
}
