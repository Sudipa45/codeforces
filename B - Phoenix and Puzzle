#include<bits/stdc++.h>
using namespace std;
int main()
{
int t;
// freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
   cin>>t;
while(t--)
{
    int n,flag=0;
    double checker,checker1;
    cin>>n;
     checker=sqrt(n*2);
     checker1=sqrt(n);
    //if(n==0)
    //{cout<<"NO"<<endl;
    //continue;}
     if((checker==int(checker))||((checker1==int(checker1))&&(n%2==0)))cout<<"YES"<<endl;
    else cout<<"NO"<<endl;


}




    return 0;
}
