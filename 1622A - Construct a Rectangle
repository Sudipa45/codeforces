#include<bits/stdc++.h>
using namespace std;
int main()
{
int t,i,arr[3];
// freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
   cin>>t;
while(t--)
{
   for(i=0;i<3;i++)
    cin>>arr[i];
   sort(arr,arr+3);
if((arr[0]==arr[1])&&arr[2]%2==0)cout<<"YES"<<endl;
else if((arr[2]==arr[1])&&arr[0]%2==0)cout<<"YES"<<endl;
else if((arr[2]-arr[1])==arr[0])cout<<"YES"<<endl;
else cout<<"NO"<<endl;
}




    return 0;
}
