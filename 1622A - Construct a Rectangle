#include<bits/stdc++.h>
using namespace std;
int main()
{
int t,i,arr[3];
// freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
   cin>>t;
while(t--)
{
    double check1,check,t,t1;
   for(i=0;i<3;i++)
    cin>>arr[i];
   sort(arr,arr+3);
   t=arr[2];
   t1=arr[0];
     check = t/2;
     check1 =t1/2;
if((arr[0]==arr[1])&&check==(int)(check))cout<<"YES"<<endl;
else if((arr[2]==arr[1])&&check1==(int)(check1))cout<<"YES"<<endl;
else if((arr[2]-arr[1])==arr[0])cout<<"YES"<<endl;
else cout<<"NO"<<endl;
}




    return 0;
}
